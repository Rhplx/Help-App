{
  "expo": {
    "name": "help",
    "slug": "help",
    "version": "3.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mx.helpapp",
      "infoPlist": {
  "NSCameraUsageDescription": "This app uses the camera to get the ID and profile picture of user",
  "NSPhotoLibraryAddUsageDescription": "This app saves photos to get the ID and profile picture of user",
  "NSPhotoLibraryUsageDescription": "This app access photos to get the ID and profile picture of user"
}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 5,
      "package": "mx.helpapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
